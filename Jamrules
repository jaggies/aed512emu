# These defaults will be overwritten by target/$(TARGET)
C++FLAGS += -std=c++11 ;
#DEFINES += AED767 ;
#DEFINES += USE_CPU=mos6502 ;
DEFINES += USE_CPU=CPU6502 ;
CCFLAGS += -Wall ;
C++FLAGS += $(CCFLAGS) -std=c++11 ;
OPTIM = -g -O3 ;
LINK = $(C++) $(OPTIM) ;
LINKLIBS += -L$(TOP)/build/$(OSFULL) ;
ALL_LOCATE_TARGET = $(TOP)/build/$(OSFULL) ;

